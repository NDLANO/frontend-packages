    "@ndla/licenses": "workspace:^",
    "@ndla/primitives": "workspace:^",
    "@ndla/styled-system": "workspace:^"
  },
  "devDependencies": {
    "@ndla/preset-panda": "workspace:^",
    "@ndla/types-backend": "^1.0.50",
    "@pandacss/dev": "^0.53.6"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
