{
  "name": "@ndla/image-search",
  "type": "module",
  "version": "11.0.128-alpha.0",
  "description": "A simple library for searching images from NDLA",
  "license": "GPL-3.0",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./es/index.mjs",
