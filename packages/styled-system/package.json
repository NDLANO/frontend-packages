    "./types": {
      "types": "./types/index.d.ts",
      "require": "./types/index.js",
      "import": "./types/index.js"
    },
    "./patterns": {
      "types": "./patterns/index.d.ts",
      "require": "./patterns/index.js",
      "import": "./patterns/index.js"
    },
    "./recipes": {
      "types": "./recipes/index.d.ts",
      "require": "./recipes/index.js",
      "import": "./recipes/index.js"
    },
    "./jsx": {
      "types": "./jsx/index.d.ts",
      "require": "./jsx/index.js",
      "import": "./jsx/index.js"
